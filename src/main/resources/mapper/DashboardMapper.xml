<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.co.info.ais.asm.mapper.DashboardMapper">

<!-- 資産タイプ別現況 -->
	<select id="possession" resultType="jp.co.info.ais.asm.domain.Dashboard">
	SELECT COUNT(asset_number)typeCnt
		, type_code
		, code_detail_name
			FROM asset
			JOIN code_detail
			WHERE  code_detail.code_master_id = "004" AND code_detail.code_detail_id = asset.type_code
			GROUP BY type_code, code_detail_name;
	</select>

